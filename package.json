{
  "name": "dashboard-pwa",
  "version": "0.1.0",
  "description": "A Progressive Web App to create mobile dashboards using D3js.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "concurrently -k -p \"[{name}]\" -n \"Node\" -c \"green.bold\" \"npm run watch-node\" \"npm run browser-sync\"",
    "watch-node": "nodemon dist/server.js",
    "browser-sync": "browser-sync start --proxy 'http://localhost:3000/' --port 1337 --files ./**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nilshoell/dashboard-pwa.git"
  },
