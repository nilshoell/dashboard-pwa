  "bugs": {
    "url": "https://github.com/nilshoell/dashboard-pwa/issues"
  },
  "homepage": "https://github.com/nilshoell/dashboard-pwa#readme",
  "dependencies": {
    "d3": "^6.6.1",
    "express": "^4.17.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
